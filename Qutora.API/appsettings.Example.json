{
  "Jwt": {
    "Issuer": "QutoraAPI",
    "Audience": "QutoraAPI",
    "ExpiryInMinutes": 60
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "https://yourdomain.com",
      "https://www.yourdomain.com"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "_EnvironmentVariables": {
    "_comment": "Set these as environment variables:",
    "ConnectionStrings__DefaultConnection": "Server=YOUR_SERVER;Database=Qutora;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True",
    "Database__Provider": "SqlServer",
    "Jwt__Key": "YOUR_JWT_SECRET_KEY_AT_LEAST_32_CHARACTERS_LONG"
  }
} 